  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ryu-g/jonbebe/issues"
  },
  "homepage": "https://github.com/ryu-g/jonbebe#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "git-rev-sync": "^2.0.0",
    "parcel-bundler": "^1.12.4",
    "pug": "^2.0.4",
    "sass": "^1.26.3"
  },
  "devDependencies": {
    "ftp-deploy": "^2.3.6"
  }
}
